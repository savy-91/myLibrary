{
  "name": "mylibrary",
  "version": "1.0.0",
  "description": "An ebook library manager using Vue, NuxtJS, Node, Express, Knex, MySQL and the Google Books API. This is a playground app, use this code with caution!",
  "author": "Sergio Castro <sergio.castro.vale@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "fs": "0.0.1-security",
    "iview": "^2.6.0",
    "knex": "^0.13.0",
    "mysql": "^2.15.0",
    "nuxt": "^1.0.0-rc3",
    "request": "^2.83.0",
    "source-map-support": "^0.4.15",
    "vue-moment": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
